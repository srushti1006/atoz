{
    "manifest_version": 3,
    "name": "Insta2Amazon",
    "version": "1.0",
    "permissions": ["activeTab", "scripting", "storage"],
    
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "assets/logo.png",   
        "48": "assets/logo.png",   
        "128": "assets/logo.png"   
      }
    },
  
    "content_scripts": [
      {
        "matches": ["*://www.instagram.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  